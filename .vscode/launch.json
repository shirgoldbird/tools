        {
            "type": "node",
            "request": "launch",
            "name": "SearchSDP",
            "program": "${workspaceFolder}/searchSDP.js",
            "args": [
                "-p",
                "dest"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "SearchContent",
            "program": "${workspaceFolder}/searchContent.js",
            "args": [
                "~~~~~",
                "-p",
                "dest"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "extractRepoListFromExcel",
            "program": "${workspaceFolder}/extractRepoListFromExcel.js",
            "args": [
                "repos.xlsx",
                "--output",
                "test.txt"
